{
    "name": "gafit/laravel-web-tinker",
    "description": "Artisan Tinker in your browser",
    "keywords": [
        "gafit",
        "web-tinker",
        "laravel",
        "tinker",
        "debug",
        "development"
    ],
    "homepage": "https://github.com/gafit/laravel-web-tinker",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.5",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\WebTinker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\WebTinker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\WebTinker\\WebTinkerServiceProvider"
            ]
        }
    }
}
